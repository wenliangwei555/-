<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:th="http://www.thymeleaf.org">
	<head th:include="commonTemplate :: header">
	</head>
	<body class="bgcolor-body">
		<div id="page">
			<div th:include="commonTemplate :: headerDiv"></div>
			<div id="content" class="layout">
				<div class="grid-s5m0">
					<div class="col-main">
						<div class="main-wrap">
							<div class="skin-box">
								<div class="skin-box-bd">
									<div class="cur-position">
										<span><a href="javascript:void(0)">信息管理</a> </span>
										<span>&gt;</span>
										<span><a th:href="@{owner/showList.ht}">流程信息管理</a> </span>
										<span>&gt;</span>
										<span><a href="javascript:void(0)">流程信息</a> </span>
									</div>
									<!-- ==================================== 页面内容开始 ============================= -->
									<div class="cont-box">
										<div class="JsW skin-box-bd" data-widget-type="tab" data-widget-config="{'activeIndex':0}">
											<div class="skin-bd-box">
												<div class="list-table">
													<div class="list-table-cont">
														<div class="table6">
															<form id="basefrm" name="basefrm"  th:action="@{engine/engineInfoStart.ht}" method="post" >
																 
																<table>
																	<tr>
																		<td>
																			<div class="left">
																				<div class="td1">
																					<span class="red">*</span><span>选择流程：</span>
																				</div>
																				<div class="td2">
																					<div class="control-css3select css3select-icon-hide">
																						<select id="deploymentId" name="deploymentId"  class="radius3">
																							<option></option>
																							<option th:each="info:${deployments}" th:if="${deployments != null}" th:value="${info.id}" th:text="${info.name}"></option>
																						</select>
																					</div>
																				</div>
																			</div>
																		</td>
																	</tr>
																</table>
																<div class="redbutton control-button JsW" data-control-type="button" data-control-config="{'buttonCls':'button'}">
																	<span> <input value="确认" type="submit"  class="radius5 button ks-pop-close1" /> </span>
																	<span class="grey"> <input value="取消" type="button" onclick="returnBack();" class="radius5 button ks-pop-close1" /> </span>
																</div>
															</form>
														</div>
													</div>
												</div>
											</div>
										</div>
									</div>
									<!-- ============================= 页面内容 结束============================= -->
								</div>
							</div>
						</div>
					</div>
					<div th:include="commonTemplate :: leftMenus"></div>
				</div>
			</div>
			<div th:include="commonTemplate :: footer"></div>
		</div>
		<div th:include="commonTemplate :: processBar" />
	<script th:inline="javascript"> 
	
		</script>
	</body>
</html>