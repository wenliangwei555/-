		function closeEva(ecID) {
			var logUrl = [[@{'/eva/closeEva.ht?ecId='}]]+ecID;
			$.ajax({
				url: logUrl,
				type:'post',
				success:function(data){
					if (data.flag == "success") {
						_alert("关闭成功");
						 var succ=[[@{'/eva/selectEvaConPage.ht'}]]; 
						locationHref(succ);
					}  else {
						_alert("关闭失败");
					}
				}
			})
		}	

	/*]]>*/
