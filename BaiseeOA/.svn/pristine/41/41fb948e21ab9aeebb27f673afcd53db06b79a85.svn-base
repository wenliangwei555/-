													<td class="col1" baisee:ctrl="ZSXYGL_XYSC">
																<div class="JsW control-checkbox"
																	data-control-type="checkbox">
																	<input type="checkbox" name="ids"
																		th:value="${info.stuId}" />
																</div>
															</td>

															<td class="col3"><input type="hidden"
																th:value="${info.stuId}" name="studentId" />
