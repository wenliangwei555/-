package cn.baisee.oa.model.tuition;
/**
 * 学费优惠表
 * @author LiFQ
 *
 */
public class TuitionDiscount {
	private String tuId;//学费主键
	
	public String getTuId() {
		return tuId;
	}
	public void setTuId(String tuId) {
		this.tuId = tuId == null ? null: tuId.trim();
	}
	/**
	 * 学费规则名称
	 */
	private String tuDiName;
	/*
	 * 学费优惠表主键
	 */
	private String tuDiId;//TU_DI_ID
	/*
	 * 优惠类型（钱/物质）
	 */
	private String tuDiType;//TU_DI_TYPE
	/*
	 * 优惠值（300元/硬盘）
	 */
	private String tuDiThing;//TU_DI_THING
	/*
	 * 优惠的钱，减去多少钱
	 */
	private String tuDiReduce;//TU_DI_REDUCE
	
	/*
	 * 最后更新时间
	 */
	private String tuDiUpdateTime;//UPDATE_TIME
	
	public String getTuDiName() {
		return tuDiName;
	}
	public void setTuDiName(String tuDiName) {
		this.tuDiName = tuDiName;
	}
	public String getTuDiReduce() {
		return tuDiReduce;
	}
	public void setTuDiReduce(String tuDiReduce) {
		this.tuDiReduce =tuDiReduce == null ? null: tuDiReduce.trim();
	}
	public String getTuDiId() {
		return tuDiId;
	}
	public void setTuDiId(String tuDiId) {
		this.tuDiId = tuDiId == null ? null: tuDiId.trim();
	}
	public String getTuDiType() {
		return tuDiType;
	}
	public void setTuDiType(String tuDiType) {
		this.tuDiType = tuDiType == null ? null: tuDiType.trim();
	}
	public String getTuDiThing() {
		return tuDiThing;
	}
	public void setTuDiThing(String tuDiThing) {
		this.tuDiThing = tuDiThing == null ? null: tuDiThing.trim();
	}
	public String getTuDiUpdateTime() {
		return tuDiUpdateTime;
	}
	public void setTuDiUpdateTime(String tuDiUpdateTime) {
		this.tuDiUpdateTime = tuDiUpdateTime == null ? null: tuDiUpdateTime.trim();
	}
	@Override
	public String toString() {
		return "TuitionDiscount [tuId=" + tuId + ", tuDiId=" + tuDiId + ", tuDiType=" + tuDiType + ", tuDiThing="
				+ tuDiThing + ", tuDiReduce=" + tuDiReduce + ", tuDiUpdateTime=" + tuDiUpdateTime + "]";
	}
	
	
	

	
	
}
