package cn.baisee.oa.model.message;

import java.util.Date;

/**
 * 消息模板
 * 
 * @author apple
 */
public class BaiseeMessageTemplate {

	
	private Long id;
	/**
	 * 标题
	 */
	private String title;
	/**
	 * 内容
	 */
	private String content;
	/**
	 * 创建时间
	 */
	private Date createTime;
	/**
	 * 发送成功次数
	 */
	private Integer sendSuccessCount;
	/**
	 * 发送失败次数
	 */
	private Integer sendFailCount;
	/**
	 * 是否发送到微信{0否、1是}
	 */
	private String isSendWechat;
	/**
	 * 是否发送到邮件：{0否、1是}
	 */
	private String isSendEmail;
	/**
	 * 是否发送到PC:{0否、1是}
	 */
	private String isSendPc;
	/**
	 * 消息类型：1系统消息、2工作通知
	 */
	private String type;
	/**
	 * 发送次数
	 */
	private Integer sendCount;
	/**
	 * 发送部门列表
	 */
	private String sendDeptList;
	/**
	 * 发送角色列表
	 */
	private String sendRoleList;
	/**
	 * 发送用户列表
	 */
	private String sendUserList;

	/**
	 * 发送人
	 */
	private String fromUserId;
	/**
	 * 发送人姓名
	 */
	private String fromUserName;
	
	public BaiseeMessageTemplate(){
		//this.sendCount = 0 ;
		this.createTime = new Date();
	}
	
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public Date getCreateTime() {
		return createTime;
	}
	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}
	public Integer getSendSuccessCount() {
		return sendSuccessCount;
	}
	public void setSendSuccessCount(Integer sendSuccessCount) {
		this.sendSuccessCount = sendSuccessCount;
	}
	public Integer getSendFailCount() {
		return sendFailCount;
	}
	public void setSendFailCount(Integer sendFailCount) {
		this.sendFailCount = sendFailCount;
	}
	public String getIsSendWechat() {
		return isSendWechat;
	}
	public void setIsSendWechat(String isSendWechat) {
		this.isSendWechat = isSendWechat;
	}
	public String getIsSendEmail() {
		return isSendEmail;
	}
	public void setIsSendEmail(String isSendEmail) {
		this.isSendEmail = isSendEmail;
	}
	public String getIsSendPc() {
		return isSendPc;
	}
	public void setIsSendPc(String isSendPc) {
		this.isSendPc = isSendPc;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public Integer getSendCount() {
		return sendCount;
	}
	public void setSendCount(Integer sendCount) {
		this.sendCount = sendCount;
	}
	public String getSendDeptList() {
		return sendDeptList;
	}
	public void setSendDeptList(String sendDeptList) {
		this.sendDeptList = sendDeptList;
	}
	public String getSendRoleList() {
		return sendRoleList;
	}
	public void setSendRoleList(String sendRoleList) {
		this.sendRoleList = sendRoleList;
	}
	public String getSendUserList() {
		return sendUserList;
	}
	public void setSendUserList(String sendUserList) {
		this.sendUserList = sendUserList;
	}
	public String getFromUserId() {
		return fromUserId;
	}
	public void setFromUserId(String fromUserId) {
		this.fromUserId = fromUserId;
	}
	public String getFromUserName() {
		return fromUserName;
	}
	public void setFromUserName(String fromUserName) {
		this.fromUserName = fromUserName;
	}

}
