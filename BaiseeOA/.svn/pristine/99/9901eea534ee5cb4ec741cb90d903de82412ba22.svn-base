<script type="text/javascript">
    /*<![CDATA[*/
    function save() {
        ids1 = new Array();
        $("input[name='ids']:checked").each(function(i){
            ids1[i]=$(this).val();
        })
        if (ids1=="" || ids==null || ids1==undefined){
            _alert("请选择学生！")
            return false;
        }
        ids = new Array();
        $("input[name='ids']:checked").each(function(i){
            ids[i]=$(this).val();
        })
        var cid = $("#claId").val();
        $.ajax({
            async: false,
            url:basePath+"/stuevaluate/updatMerger.ht",
        data:{
            "id":ids.toString(),"cid":cid
        },
        type:'post',
            success:function(data){
            console.log(data)
            if (data.code == "000") {
                stuAuditionStartTime();
            }else {
                _alert("合班失败！")
            }
        }
    })
    }
    $(function(){
        $('.search-bar .dropdown-menu a').click(function() {
            var field = $(this).data('field') || '';
            $('#search-field').val(field);
            $('#search-btn').html($(this).text() + ' <span class="caret"></span>');
        });
    });
    function stuAuditionStartTime() {
        var stuStartTime = $("#stuStartTime").val();
        var stuEndTime = $("#stuEndTime").val();
        if (stuStartTime > stuEndTime) {
            _alert("入学结束时间不得早于开始时间");
            return false;
        }
        document.getElementById("frm").submit();
    }
    function merger() {

    }
    /*]]>*/
</script>
